class Solution {
    public String findDifferentBinaryString(String[] nums) {
        StringBuffer sb = new StringBuffer();
        int j=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i].charAt(j)=='0'){
                sb.append('1');
            }else{
                sb.append('0');
            }
            j++;
        }
        return sb.toString();
    }
}
